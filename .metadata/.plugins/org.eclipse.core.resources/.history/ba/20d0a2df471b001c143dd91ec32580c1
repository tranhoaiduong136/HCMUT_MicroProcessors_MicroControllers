#include "main.h"
#include "matrix_led.h"

static uint8_t matrixLEDBuffer[NUMS_MLED];
uint8_t colConversionGATE[5] = {CLK11,CLK3,CLK4,CLK10,CLK6};
uint8_t rowConversionGATE[7] = {CLK9,CLK12,CLK8,CLK5,CLK1,CLK7,CLK2};


void matrixLEDInit(void){
	HAL_GPIO_WritePin(GROUP_PIN,rowConversionGATE[1],OFF);
	HAL_GPIO_WritePin(GROUP_PIN, colConversionGATE[1], ON);
}
void updatematrixLEDBuffer(uint8_t val,int idx){
	matrixLEDBuffer[idx] = val;
}
void matrixLEDDriver(int idx){

}
void clearAllClock(void){

}
